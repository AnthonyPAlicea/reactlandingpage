 document.addEventListener('DOMContentLoaded', function () {
  function startCountdown(selector) {
    const timers = document.querySelectorAll(selector);

    timers.forEach(timer => {
      const endTime = new Date(timer.getAttribute('data-end-time'));
      const daysElem = timer.querySelector('.days');
      const hoursElem = timer.querySelector('.hours');
      const minutesElem = timer.querySelector('.minutes');
      const secondsElem = timer.querySelector('.seconds');

      function updateTimer() {
        const now = new Date();
        const diff = endTime - now;

        if (diff <= 0) {
          // Hide the timer and stop the interval
          timer.style.display = 'none';
          clearInterval(activeIntervals.get(timer));
          activeIntervals.delete(timer);
          return;
        }

        const days = Math.floor(diff / (1000 * 60 * 60 * 24));
        const hours = Math.floor((diff / (1000 * 60 * 60)) % 24);
        const minutes = Math.floor((diff / (1000 * 60)) % 60);
        const seconds = Math.floor((diff / 1000) % 60);

        daysElem.textContent = String(days).padStart(2, '0');
        hoursElem.textContent = String(hours).padStart(2, '0');
        minutesElem.textContent = String(minutes).padStart(2, '0');
        secondsElem.textContent = String(seconds).padStart(2, '0');
      }

      if (activeIntervals.has(timer)) {
        return; // Avoid duplicate intervals
      }

      const interval = setInterval(updateTimer, 1000);
      activeIntervals.set(timer, interval);
      updateTimer(); // Initial run to set the timer immediately
    });
  }

  const activeIntervals = new Map();
  //startCountdown('.countdown');
});
</script> 
